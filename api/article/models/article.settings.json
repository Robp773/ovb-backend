{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Mindset",
        "Culture",
        "Leadership",
        "Team",
        "Process",
        "Real_Life_Applications"
      ],
      "required": true
    },
    "drills": {
      "via": "articles",
      "collection": "drill"
    },
    "ropes_course_activities": {
      "via": "articles",
      "collection": "ropes-course-activity",
      "dominant": true
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "sections.tags"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "main_media": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "description": {
      "type": "string",
      "required": true
    }
  }
}
